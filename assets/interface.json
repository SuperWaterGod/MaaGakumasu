{
  "name":"MaaGakumasu",
  "version":"0.0.1",
  "mirrorchyan_rid":null,
  "mirrorchyan_multiplatform":false,
  "url":"https://github.com/SuperWaterGod/MaaGakumasu",
  "custom_title":"MaaGakumasu",
  "controller":[
    {
      "name":"官服",
      "type":"Adb"
    }
  ],
  "resource":[
    {
      "name":"官服",
      "path":[
        "{PROJECT_DIR}/resource/base"
      ]
    }
  ],
  "agent":{
    "child_exec":"python",
    "child_args":[
      "{PROJECT_DIR}/agent/main.py"
    ]
  },
  "task":[
    {
      "name":"启动游戏",
      "entry":"StartUp",
      "check":true,
      "doc":""
    },
    {
      "name":"收取活动费",
      "entry":"ActivityFee",
      "check":true,
      "doc":""
    },
    {
      "name":"每日挑战",
      "entry":"Challenge",
      "option":[
        "选择挑战第几位"
      ],
      "check":true,
      "doc":""
    },
    {
      "name":"安排工作",
      "entry":"Work",
      "option":[
        "自动安排工作",
        "迷你演唱会工作时长",
        "直播活动工作时长",
        "指定迷你演唱会Idol",
        "指定直播活动Idol"
      ],
      "check":true,
      "doc":""
    },
    {
      "name":"社团活动",
      "entry":"Society",
      "option":[
        "请求物品",
        "捐赠"
      ],
      "check":true,
      "doc":""
    }
  ],
  "option":{
    "选择挑战第几位":{
      "cases":[
        {
          "name":"第一位",
          "pipeline_override":{
            "ChallengeChoose":{
              "index":0
            }
          }
        },
        {
          "name":"第二位",
          "pipeline_override":{
            "ChallengeChoose":{
              "index":1
            }
          }
        },
        {
          "name":"第三位",
          "pipeline_override":{
            "ChallengeChoose":{
              "index":2
            }
          }
        }
      ]
    },
    "自动安排工作":{
      "cases":[
        {
          "name":"Yes",
          "pipeline_override":{
            "WorkChooseShow":{
              "custom_recognition":"WorkChooseAuto"
            },
            "WorkChooseLive":{
              "custom_recognition":"WorkChooseAuto"
            }
          }
        },
        {
          "name":"No",
          "pipeline_override":{
            "WorkChooseShow":{
              "custom_recognition":"WorkChooseIdol"
            },
            "WorkChooseLive":{
              "custom_recognition":"WorkChooseIdol"
            }
          }
        }
      ]
    },
    "迷你演唱会工作时长":{
      "cases":[
        {
          "name":"4 小时",
          "pipeline_override":{
            "WorkChooseShowTime":{
              "expected":"4時間"
            }
          }
        },
        {
          "name":"8 小时",
          "pipeline_override":{
            "WorkChooseShowTime":{
              "expected":"8時間"
