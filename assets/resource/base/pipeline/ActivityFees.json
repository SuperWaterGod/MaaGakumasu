{
  "ActivityFee": {
    "recognition": {
      "type": "DirectHit",
      "param": {}
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "next": [
      "ActivityFeeHomeFlag",
      "[JumpBack]ReturnHome",
      "[JumpBack]BackHome",
      "[JumpBack]CloseRoundButton"
    ]
  },
  "ActivityFeeHomeFlag": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": [
          "home.png",
          "home_1.png"
        ]
      }
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "next": [
      "ActivityFeeClick"
    ]
  },
  "ActivityFeeClick": {
    "recognition": {
      "type": "OCR",
      "param": {
        "expected": [
          "活動費"
        ],
        "replace": [
          [
            "费",
            "費"
          ],
          [
            "动",
            "動"
          ]
        ],
        "roi": [
          0,
          520,
          160,
          130
        ]
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "next": [
      "ActivityFeeClose",
      "ActivityWithoutFee"
    ]
  },
  "ActivityFeeClose": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "close.png"
      }
    },
    "action": {
      "type": "Click",
      "param": {}
    },
    "next": [
      "HomeFlag"
    ],
    "focus": {
      "Node.Action.Succeeded": "[color:green]活动费用已领取[/color]"
    }
  },
  "ActivityWithoutFee": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": "activity_without_fee.png"
      }
    },
    "action": {
      "type": "DoNothing",
      "param": {}
    },
    "next": [
      "HomeFlag"
    ],
    "focus": {
      "Node.Action.Succeeded": "[color:orange]无活动费用可领取[/color]"
    }
  }
}