    "post_wait_freezes":200,
    "next":[
      "ShoppingDailyExchangeMoney"
    ]
  },
  "ShoppingDailyExchangeMoney":{
    "recognition":{
      "type":"TemplateMatch",
      "param":{
        "template":"shopping_money.png",
        "roi":[
          50,
          1060,
          300,
          70
        ]
      }
    },
    "action":{
      "type":"Custom",
      "param":{
        "custom_action":"ShoppingDailyExchangeMoneyAuto"
