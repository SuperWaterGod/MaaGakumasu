  "StartUpHomeFlag": {
    "recognition": {
      "type": "TemplateMatch",
      "param": {
        "template": [
          "home.png",
          "home_1.png"
        ]
      }
    },
    "action": {
      "type": "Custom",
      "param": {
        "custom_action": "UseCounter",
